{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "extends": ["@schematics/angular"],
  "schematics": {
    "ng-add": {
      "description": "Install ngx-onsenui",
      "factory": "./ng-add/index",
      "schema": "./ng-add/schema.json"
    },
    "ng-add-setup-project": {
      "description": "Set up the specified project after the ng-add dependencies have been installed",
      "private": true,
      "factory": "./ng-add/setup-project",
      "schema": "./ng-add/schema.json"
    },
    "page": {
      "description": "Create a page component",
      "factory": "./page/index",
      "schema": "./page/schema.json"
    }
  }
}
